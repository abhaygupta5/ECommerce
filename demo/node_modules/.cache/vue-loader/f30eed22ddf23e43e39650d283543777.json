ce * item.Quantity\n        },\n        prepareTableData() {\n            this.items.map(item => {\n                item.SubTotal = this.getSubTotal(item)\n\n            })\n        },\n        remove(item, index) {\n               console.log('ITEM:', item)\n                this.items.splice(index,1)\n        }\n    },\n    created() {\n        this.prepareTableData()\n    }\n    \n}\n</script>\n\n<style>\n\n#Cart{\n    margin-top: 100px;\n    margin-left: 30px;\n}\n.remove-component{\n    margin-top: 106px;\n}\n.remove{\n    margin-top: 0px;\n    position: absolute;\n    margin-left: 790px;\n    \n}\n.btn{\n        padding: 9px;\n    margin: 3px\n}\n.summary{\n    \n}\n\n</style>\n"]}]}